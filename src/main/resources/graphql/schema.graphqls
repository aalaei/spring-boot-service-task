type Query{
    services: [Service]
    service(id: ID!): Service
}
type Service{
    id: ID!
    criticalText: String
    resources: [Resource]
}
type Resource{
    id: ID!
    criticalText: String
    owners: [Owner]
}
type Owner{
    id: ID!
    criticalText: String
    name: String
    accountNumber: String
    level: Int
}
